{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "name": "material/card-query",
  "category": "material",
  "attributes": {
    "showExcerpt": {
      "type": "boolean",
      "default": true
    },
    "showDate": {
      "type": "boolean",
      "default": true
